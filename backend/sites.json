dding: 60px 20px;\n            background-color: #4B60A6;\n            color: #FFF8F3;\n        }\n        \n        .featured-products h2 {\n            font-size: 36px;\n            margin-bottom: 40px;\n            text-align: center;\n        }\n        \n        .product-grid {\n            display: grid;\n            grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));\n            grid-gap: 40px;\n        }\n        \n        .product-card {\n            text-align: center;\n        }\n        \n        .product-card img {\n            width: 100%;\n            max-height: 300px;\n            object-fit: cover;\n            border-radius: 4px;\n            margin-bottom: 20px;\n        }\n        \n        .product-card h3 {\n            font-size: 24px;\n            margin-bottom: 10px;\n        }\n        \n        .product-card p {\n            font-size: 18px;\n            margin-bottom: 20px;\n        }\n        \n        /* Contact Form Styles */\n        .contact {\n            padding: 60px 20px;\n            background-color: #FFF8F3;\n        }\n        \n        .contact h2 {\n            font-size: 36px;\n            margin-bottom: 40px;\n            text-align: center;\n        }\n        \n        form {\n            max-width: 600px;\n            margin: 0 auto;\n        }\n        \n        form label {\n            display: block;\n            font-size: 18px;\n            margin-bottom: 10px;\n        }\n        \n        form input, form textarea {\n            width: 100%;\n            padding: 12px;\n            font-size: 16px;\n            border: 1px solid #ccc;\n            border-radius: 4px;\n            box-sizing: border-box;\n            margin-bottom: 20px;\n        }\n        \n        form button {\n            background-color: #DB7093;\n            color: #FFF8F3;\n            padding: 12px 24px;\n            border: none;\n            border-radius: 4px;\n            font-size: 18px;\n            cursor: pointer;\n        }\n        \n        /* Footer Styles */\n        footer {\n            background-color: #4B60A6;\n            color: #FFF8F3;\n            padding: 20px;\n            text-align: center;\n            font-size: 14px;\n        }\n    </style>\n</head>\n<body>\n    <header>\n        <div class=\"logo\">Pink & Blue Boutique</div>\n        <nav>\n            <ul>\n                <li><a href=\"#\">Home</a></li>\n                <li><a href=\"#\">Shop</a></li>\n                <li><a href=\"#\">About</a></li>\n                <li><a href=\"#\">Contact</a></li>\n            </ul>\n        </nav>\n    </header>\n\n    <section class=\"hero\">\n        <h1>Discover the Latest Trends in Fashion</h1>\n        <p>Explore our curated collection of stylish clothing and accessories.</p>\n        <button class=\"cta-button\">Shop Now</button>\n    </section>\n\n    <section class=\"featured-products\">\n        <h2>Featured Products</h2>\n        <div class=\"product-grid\">\n            <div class=\"product-card\">\n                <img src=\"https://via.placeholder.com/300x300\" alt=\"Product 1\">\n                <h3>Pink Floral Dress</h3>\n                <p>$59.99</p>\n                <button class=\"cta-button\">Add to Cart</button>\n            </div>\n            <div class=\"product-card\">\n                <img src=\"https://via.placeholder.com/300x300\" alt=\"Product 2\">\n                <h3>Blue Striped Shirt</h3>\n                <p>$39.99</p>\n                <button class=\"cta-button\">Add to Cart</button>\n            </div>\n            <div class=\"product-card\">\n                <img src=\"https://via.placeholder.com/300x300\" alt=\"Product 3\">\n                <h3>Pink Polka Dot Skirt</h3>\n                <p>$49.99</p>\n                <button class=\"cta-button\">Add to Cart</button>\n            </div>\n        </div>\n    </section>\n\n    <section class=\"contact\">\n        <h2>Get in Touch</h2>\n        <form>\n            <label for=\"name\